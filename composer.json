{
  "name": "lumynus/lumynus",
  "description": "A lightweight PHP framework designed for developers who value simplicity, clarity, and performance.",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Lumynus\\Bundle\\Framework\\": "vendor/lumynus/kernel/bundle/framework/",
      "Lumynus\\Bundle\\Contracts\\": "vendor/lumynus/kernel/bundle/contracts/",
      "Lumynus\\Templates\\": "vendor/lumynus/kernel/templates/",
      "Lumynus\\Http\\": "vendor/lumynus/kernel/Http/"
    }
  },
  "authors": [
    {
      "name": "Weleny Santos",
      "email": "welenysantos@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "lumynus/kernel": "*"
  },
  "scripts": {
    "test": "phpunit"
  },
  "keywords": [
    "framework",
    "php",
    "lumynus",
    "lightweight",
    "microframework"
  ]
}
