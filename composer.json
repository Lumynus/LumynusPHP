{
  "name": "lumynus/lumynus",
  "description": "A lightweight PHP framework designed for developers who value simplicity, clarity, and performance.",
  "version": "1.0",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Lumynus\\Bundle\\Framework\\": "vendor/Lumynus/Bundle/Framework/",
      "Lumynus\\Bundle\\Contracts\\": "vendor/Lumynus/Bundle/Contracts/",
      "Lumynus\\Templates\\": "vendor/Lumynus/Templates/"
    }
  },
  "authors": [
    {
      "name": "Weleny Santos",
      "email": "welenysantos@gmail.com"
    }
